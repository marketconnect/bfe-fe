{
  "name": "file-service-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "debug:deploy": "dotenv -e .env -- bash -lc 'echo User:$DEPLOY_USER, Host:$DEPLOY_HOST, Port:$DEPLOY_PORT, Path:$DEPLOY_PATH'",
    "deploy": "dotenv -e .env -- bash -lc 'RSYNC_RSH=\"ssh -p $DEPLOY_PORT\" rsync -avz --delete ./out/ \"$DEPLOY_USER@$DEPLOY_HOST:$DEPLOY_PATH\"'",
    "preview:static": "npx serve out -l 5173"
  },
  "dependencies": {
    "@types/accept-language-parser": "^1.5.8",
    "accept-language-parser": "^1.5.0",
    "jwt-decode": "^4.0.0",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
